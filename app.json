{
  "expo": {
    "name": "GeoGather",
    "description": "An app for making observations of Emerald Ash Borer infected Ash trees",
    "slug": "geo-gather",
    "privacy": "unlisted",
    "version": "0.1.0",
    "githubUrl": "https://github.com/mstone121/GeoGather",
    "primaryColor": "#347227",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "geo-gather",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "site.mattstone.GeoGather"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
